{
  "database": {
    "rules": "database.rules.json"
  },
  "hosting": {
    "site": "devfest-sd-23",
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [
      {
        "source": "**",
        "destination": "/index.html"
      }
    ]
  },
  "functions": {
    "source": "functions",
    "runtime": "nodejs14"
  }
}
